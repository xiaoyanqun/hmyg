<tabs tabs="{{tabs}}" bindItemChange="ItemChange">
  <view class="synthesize" >
      <view  class="form_item" wx:for="{{orders}}" wx:key="order_id">
          <view class="item_row" >
            <view class="" >
              订单编号
            </view>
            <view class="row_number" >
              {{item.order_number}}
            </view>
          </view>
          <view class="item_row" >
             <view class="" >
               订单价格
             </view>
             <view class="row_price" >
               ￥{{item.total_price}}
             </view>
           </view>
           <view class="item_row">
               <view class="" >
                 订单日期
               </view>
               <view class="row_date" >
                 {{item.create_time_cn}}
               </view>
             </view>
            
        </view>
  </view>
 
</tabs>